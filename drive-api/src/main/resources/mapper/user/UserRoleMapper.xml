<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.castle.drive.user.mapper.UserRoleMapper">

    <select id="getUserRoleById" resultType="com.castle.drive.user.vo.UserRoleVo">
        select *
        from user_role
            where id = #{id}
    </select>

    <select id="getUserRolePage" parameterType="com.castle.drive.user.query.UserRoleQuery"
            resultType="com.castle.drive.user.vo.UserRoleVo">
        select *
        from user_role
    </select>

</mapper>
